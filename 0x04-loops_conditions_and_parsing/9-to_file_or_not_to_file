#!/usr/bin/env bash
# Gives information about holbertonschool file

f=holbertonschool
if [ -e "$f" ]
then
	echo "holbertonschool file exists"
	if [ -s "$f" ]
	then
		echo "holbertonschool file is not empty"
	else
		echo "holbertonschool file is empty"
	fi
else
	echo "holbertonschool file does not exists"
fi

if [ -f "$f" ]
then
	echo "holbertonschool file is a regular file"
fi
